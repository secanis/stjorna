{
    "name": "stjorna",
    "version": "2.7.4",
    "description": "STJÃ“RNA product management",
    "main": "server.cjs",
    "type": "module",
    "scripts": {
        "start": "nodemon --ignore data/ server.cjs",
        "apidoc:private": "apidoc --private true -i ./api -o ./apidoc-private/",
        "apidoc:public": "apidoc -i ./api -o ./apidoc/",
        "test": "rimraf test/testdata & rimraf reports & npm run test:coverage",
        "test:coverage": "nyc --reporter html --reporter lcovonly --report-dir reports/coverage --temp-dir reports/coverage/.nyc_output --reporter text npm run test:api",
        "test:api": "mocha test/**/*.spec.cjs --reporter mocha-multi-reporters --reporter-options configFile=test/_config.json --timeout 5000 --full-trace --exit",
        "test:sendcoverage": "cat reports/coverage/lcov.info | codacy-coverage"
    },
    "author": "matthias.baldi@secanis.ch",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/secanis/stjorna.git"
    },
    "dependencies": {
        "archiver": "^5.3.1",
        "body-parser": "^1.20.1",
        "cron": "^1.8.2",
        "excel4node": "^1.8.0",
        "express": "^4.18.2",
        "express-winston": "^4.2.0",
        "extract-zip": "^2.0.1",
        "formidable": "^3.2.4",
        "fs-temp": "^1.2.1",
        "jimp": "^0.16.2",
        "jsonwebtoken": "^9.0.0",
        "lowdb": "^1.0.0",
        "matomo-tracker": "^2.2.4",
        "moment": "^2.29.4",
        "sharp": "^0.30.7",
        "snyk": "^1.1079.0",
        "uniqid": "^5.4.0",
        "util": "^0.12.5",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "apidoc": "^0.53.1",
        "chai": "^4.3.7",
        "chai-http": "^4.3.0",
        "codacy-coverage": "^3.4.0",
        "mocha": "^10.2.0",
        "mocha-junit-reporter": "^2.2.0",
        "mocha-multi-reporters": "^1.5.1",
        "nodemon": "2.0.20",
        "nyc": "^15.1.0",
        "request": "^2.88.2",
        "rimraf": "^3.0.2"
    }
}
